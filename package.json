{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "start": "npm-run-all --parallel dev browser-sync",
    "dev": "next",
    "browser-sync": "browser-sync start --proxy \"http://localhost:3000\" --port 8080",
    "build": "next build",
    "type-check": "tsc",
