    "bootstrap": "^4.3.1",
    "isomorphic-unfetch": "3.0.0",
    "next": "^9.1.3",
    "next-images": "^1.2.0",
    "node-sass": "^4.13.0",
    "react": "^16.10.1",
    "react-bootstrap": "^1.0.0-beta.14",
    "react-dom": "^16.10.1"
  },
  "devDependencies": {
    "@types/node": "^12.7.8",
    "@types/react": "^16.9.3",
    "@types/react-dom": "^16.9.1",
    "browser-sync": "^2.26.7",
    "cross-env": "^6.0.3",
